{
	"_version": "1.3.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "hcp://supportsitetemplate/Support_Site/pageTemplates/Home/manifest.json",
		"title": "Home",
		"description": "A page layout that contains a header, footer, and three columns. The center column contains the main content, and the left and right columns contain supplemental content such as links and navigation.",
		"tags": {
			"keywords": []
		},
		"crossNavigation": {
			"inbounds": {
				"0": {
					"semanticObject": "obj",
					"action": "Home",
					"deviceTypes": {
						"desktop": true,
						"tablet": true,
						"phone": true
					}
				}
			}
		}
	},
	"sap.platform.hcp": {
		"_version": "1.2.0",
		"appName": "supportsitetemplate",
		"uri": "/Support_Site/pageTemplates/Home"
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"componentName": "cpv2.templates.Home"
	},
	"sap.cloud.portal": {
		"applicationType": "page",
		"nodes": {
		"footer": {
		
        "_version": "1.3.0",
        "sap.app": {
            "_version": "1.3.0",
            "title": "HTML Widget",
            "description": "A widget template that displays HTML files.",
            "tags": {
              "keywords": [
                "keyword"
              ]
            },
            "crossNavigation": {
              "inbounds": {
                "0":{
                  "semanticObject": "htmlwidget",
                  "action": "Display",
                  "deviceTypes": {
                    "desktop": true,
                    "tablet": true,
                    "phone": true
                  }
                }
              }
            }
        },
        "sap.platform.hcp": {
            "_version": "1.2.0",
            "appName": "portalapptemplates",
            "uri": "/htmlwidget"
          },
          "sap.ui5":{
            "_version": "1.2.0",
            "componentName": "htmlwidget.local"
          },
          "sap.cloud.portal": {
            "show.portal.settings":"false",
            "settings": {
              "markup": {
                "isRequired": "false",
                "storage_type": "ecm",
                "displayName": "HTML",
                "description": "resource_url_description",
                "id": "markup"
              },
              "resources": {
                "assets": [
                  {
                    "value": "/assets/markup/Footer_HTML.html"
                  }
                ]
              }
            }

          },
          "sap.flp": {
            "_version": "1.1.0",
            "config": {}
          },
          "sap.ui": {
            "_version": "1.3.0",
            "technology": "UI5"
          },
        "local": "true",
        "sectionAlias": "page1.footer"     
      }
		},
		"thumbnail": "preview.png"
	},
	"sap.flp": {
		"tileType": "",
		"appType": "UI5",
		"config": {
			"editable": true,
			"deletable": true,
			"theme.id": "sap_bluecrystal",
			"theme.active": "[\"sap_bluecrystal\",\"sap_hcb\"]"
		}
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5"
	}
}